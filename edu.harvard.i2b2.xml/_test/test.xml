<examples>

    <value xsi:type="i2b2:tval_valueType" concept_cd="birthdate">yellow</value>
    <value xsi:type="i2b2:nval_valueType" concept_cd="test_result">12.0</value>
    <value xsi:type="i2b2:blob_valueType" concept_cd="extradata">
        <first>Some xml blob data.</first>
    </value>


    <value xsi:type="i2b2:date_valueType" concept_cd="birthdate">2002-01-18</value>
    <value xsi:type="i2b2:tval_nval_valueType" concept_cd="test_result">
