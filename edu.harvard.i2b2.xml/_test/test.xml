        <value xsi:type="i2b2:nval_valueType" concept_cd="test_result">20</value>
    </value>


    <tval concept_cd="birthdate">yellow</tval>
    <nval concept_cd="test_result">12.0</nval>
    <blob concept_cd="extradata">
        <first>
            <second>
                <third>Something here.</third>
            </second>
        </first>
    </blob>


    <date concept_cd="birthdate">2002-01-18</date>
    <tval_nval concept_cd="test_result">
        <tval concept_cd="test_result">&gt;</tval>
        <nval concept_cd="test_result">20</nval>
    </tval_nval>


